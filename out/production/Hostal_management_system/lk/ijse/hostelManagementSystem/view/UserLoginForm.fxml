<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainAdmin2" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.hostelManagementSystem.controller.UserLoginFormController">
    <children>
        <Pane opacity="0.86" prefHeight="400.0" prefWidth="253.0" style="-fx-background-color: #1B4E5E;">
            <children>
                <Label layoutX="14.0" layoutY="48.0" text="User Name" textFill="WHITE">
                    <font>
                        <Font name="HoloLens MDL2 Assets" size="21.0" />
                    </font>
                </Label>
                <Label layoutX="14.0" layoutY="140.0" text="Password" textFill="WHITE">
                    <font>
                        <Font name="HoloLens MDL2 Assets" size="21.0" />
                    </font>
                </Label>
                <TextField layoutX="14.0" layoutY="81.0" prefHeight="37.0" prefWidth="172.0" style="-fx-background-radius: 30;">
                    <font>
                        <Font size="18.0" />
                    </font></TextField>
                <PasswordField fx:id="passwordHidden" layoutX="14.0" layoutY="170.0" prefHeight="37.0" prefWidth="172.0" style="-fx-background-radius: 30;">
                    <font>
                        <Font size="15.0" />
                    </font>
                </PasswordField>
                <TextField fx:id="passwordText" layoutX="14.0" layoutY="170.0" prefHeight="37.0" prefWidth="172.0" style="-fx-background-radius: 30;" visible="false">
                    <font>
                        <Font size="15.0" />
                    </font>
                </TextField>
                <CheckBox fx:id="checkBox" layoutX="38.0" layoutY="215.0" mnemonicParsing="false" onAction="#changeVisibility" text="Show Password" textFill="#a89b9b" />
                <Button fx:id="btnlogin" layoutX="58.0" layoutY="242.0" mnemonicParsing="false" onAction="#gofoward1" prefHeight="44.0" prefWidth="133.0" style="-fx-background-radius: 15; -fx-background-color: #132550;" text="Login" textFill="#c9baba">
                    <font>
                        <Font name="Arial" size="21.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button fx:id="btnback" layoutX="83.0" layoutY="306.0" mnemonicParsing="false" onAction="#goBack" prefHeight="37.0" prefWidth="88.0" style="-fx-background-radius: 15; -fx-background-color: #000000;" text="Close" textFill="#cdbcbc">
                    <font>
                        <Font size="15.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
